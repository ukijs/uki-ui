{
  "name": "@ukijs/ui",
  "version": "0.2.4",
  "description": "A UI toolkit using the uki.js library",
  "module": "dist/uki-ui.esm.js",
  "jsnext:main": "dist/uki-ui.esm.js",
  "scripts": {
    "example": "bash examples/run.sh",
    "build": "rollup -c",
    "lint": "eslint .",
    "dev": "rollup -c -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ukijs/uki-ui.git"
  },
  "author": "Alex Bigelow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ukijs/uki-ui/issues"
  },
  "homepage": "https://github.com/ukijs/uki-ui#readme",
  "eslintConfig": {
    "extends": "standard",
    "rules": {
      "semi": [
        2,
        "always"
      ],
      "no-extra-semi": 2,
      "semi-spacing": [
        2,
        {
          "before": false,
          "after": true
        }
      ]
    },
    "globals": {
      "globalThis": false
    }
  },
  "devDependencies": {
    "@rollup/plugin-image": "^2.0.5",
    "@rollup/plugin-json": "^4.1.0",
    "d3": "^6.2.0",
    "eslint": "^7.13.0",
    "eslint-config-semistandard": "^15.0.1",
    "eslint-config-standard": "^16.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "normalize.css": "^8.0.1",
    "rollup": "^2.33.1",
    "rollup-plugin-execute": "^1.1.1",
    "rollup-plugin-less": "^1.1.2",
    "rollup-plugin-string": "^3.0.0",
    "serve": "^11.3.2",
    "uki": "^0.7.6"
  },
  "peerDependencies": {
    "d3": "^6.2.0",
    "uki": "^0.7.6"
  },
  "optionalDependencies": {
    "golden-layout": "^1.5.9",
    "jquery": "^3.5.1",
    "less": "^3.12.2",
    "vega": "^5.17.0",
    "vega-lite": "^4.17.0"
  }
}
