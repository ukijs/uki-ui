{
  "name": "@ukijs/uki-ui",
  "version": "0.1.5",
  "description": "A UI toolkit using the uki.js library",
  "module": "dist/uki-ui.esm.js",
  "jsnext:main": "dist/uki-ui.esm.js",
  "scripts": {
    "example": "bash examples/run.sh",
    "build": "rollup -c && ls -d examples/*/ | xargs -n 1 cp -v dist/uki-ui.esm.js && ls -d examples/*/ | xargs -n 1 cp -v node_modules/uki/dist/uki.esm.js",
    "dev": "rollup -c -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ukijs/uki-ui.git"
  },
  "author": "Alex Bigelow",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ukijs/uki-ui/issues"
  },
  "homepage": "https://github.com/ukijs/uki-ui#readme",
  "devDependencies": {
    "normalize.css": "^8.0.1",
    "rollup": "^2.17.1",
    "rollup-plugin-less": "^1.1.2",
    "rollup-plugin-string": "^3.0.0",
    "serve": "^11.3.2"
  },
  "peerDependencies": {
    "d3": "^5.16.0"
  },
  "optionalDependencies": {
    "golden-layout": "^1.5.9",
    "less": "^3.11.3"
  },
  "dependencies": {
    "uki": "^0.6.6"
  }
}
